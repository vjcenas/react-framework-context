{
  "name": "react-framework-context",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=12.14.1"
  },
  "scripts": {
    "ts:css": "tsm src/**/*.module.scss -s -c -p",
    "start": "concurrently \"npm:ts:css -- -w\" \"react-scripts start\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage",
    "lint": "yarn run lint:ts && yarn run lint:css",
    "lint:ts": "tsc && yarn lint:eslint",
    "lint:eslint": "eslint --ext=ts,tsx \"src/**/*.{ts,tsx}\"",
    "lint:css": "stylelint --syntax scss \"src/**/*.scss\"",
    "storybook": "start-storybook -p 4000 -s public"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "axios": "^0.19.2",
    "classnames": "^2.2.6",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-localization": "^1.0.15",
    "react-router-dom": "^5.2.0",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.0.19",
    "@storybook/addon-links": "^6.0.19",
    "@storybook/addon-viewport": "^6.0.19",
    "@storybook/addons": "^6.0.19",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^6.0.19",
    "@types/axios": "^0.14.0",
    "@types/classnames": "^2.2.10",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/node-sass": "^4.11.1",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^5.1.5",
    "@types/yup": "^0.29.6",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "concurrently": "^5.3.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^6.3.1",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-unicorn": "^15.0.1",
    "node-sass": "^4.14.1",
    "react-scripts": "3.4.1",
    "stylelint": "^13.6.1",
    "stylelint-config-css-modules": "^2.2.0",
    "stylelint-config-standard": "^20.0.0",
    "typed-scss-modules": "^2.0.1",
    "typescript": "~3.7.2"
  }
}
